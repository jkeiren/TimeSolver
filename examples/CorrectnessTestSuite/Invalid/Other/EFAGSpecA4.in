CLOCKS :{x1}
CONTROL :{p1(3)}
PREDICATE: {X1, X2}
START: X1
EQUATIONS: {
1: mu X1 = X2
	  || (\exists time(X1
	  || ( (p1 == 0) && (X1[p1=1]))
	  || ( (p1 == 1 && x1 > 2) && (X1[p1=2]))
	  || ( (p1 == 2) && (X1[p1=0][x1])) ) )
2: nu X2 = (p1 == 2)
	  && (\forall time(X2
	  && ( (p1 == 0) -> (X2[p1=1]))
	  && ( (p1 == 1 && x1 > 2) -> (X2[p1=2]))
	  && ( (p1 == 2) -> (X2[p1=0][x1])) ) )
}
INVARIANT:
	p1 == 0 -> x1 <= 5
	p1 == 1 -> x1 <= 5
	p1 == 2 -> x1 <= 5
