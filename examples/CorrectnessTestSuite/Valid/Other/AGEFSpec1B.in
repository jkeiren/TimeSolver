CLOCKS :{x1, x2}
CONTROL :{p1(2)}
PREDICATE: {X1, X2}
START: X1
EQUATIONS: {
1: nu X1 = X2
	  && (\forall time(X1
	  && ( (p1 == 0 && x2 > 1) -> (X1[p1=0][x2]))
	  && ( (p1 == 0) -> (X1[p1=1][x1,x2]))
	  && ( (p1 == 1 && x1 >= 1) -> (X1[p1=1][x1])) ) )
2: mu X2 = x1 == 1
	|| (\exists time(X2
	||( (p1 == 0 && x2 > 1) && (X2[p1=0][x2]))
	||( (p1 == 0) && (X2[p1=1][x1,x2]))
	||( (p1 == 1 && x1 >= 1) && (X2[p1=1][x1])) ))
	}
INVARIANT:
	p1 == 0 -> x1 <= 2
	p1 == 1 -> x1 <= 2
